<%= form_for @dish do |f| %>

  <%= f.label :title%>
  <%= f.text_field :title %>

  <%= f.label :image_url%>
  <%= f.text_field :image_url %>

  <%= f.label :upload_file %>
  <%= f.file_field :photo %>

  <%= f.label :remote_photo_url %>
  <%= f.text_field :remote_photo_url %>

  <%= f.collection_check_boxes :tag_ids, Tag.all, :id, :name %>

  <%= f.submit %>

<% end %>
