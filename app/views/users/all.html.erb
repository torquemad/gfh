<h2>All Users</h2>

<%= link_to 'new user', new_user_path %>

<% @users.each do |user| %>

  <p>
    <%= link_to user.email, user %> <br> 
    <%= link_to 'edit', edit_user_path(user) %>

    <%= form_for user, method: :delete do |f| %>
      <%= f.submit 'Delete' %>
    <% end %>

  </p>

<% end %>